void CG_FILL_FULL(int nSP, int nColorR, int nColorG, int nColorB)
{
	size_t siz;
	SP_GET_SIZE(nSP, siz.w, siz.h);
	CG_FILL(nSP, 0, 0, siz.w, siz.h, nColorR, nColorG, nColorB);
}

void CG_ROTATE_RGB(int nSP, int nX, int nY, int nWidth, int nHeight, int nRotateType)
{
	SACT2.CG_RotateRGB(nSP, nX, nY, nWidth, nHeight, nRotateType);
}

