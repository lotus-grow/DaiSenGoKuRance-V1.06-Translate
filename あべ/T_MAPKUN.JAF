}

int tagMapKuniManager::setColor(int nMapNo, int nColorNo)
{
	ref tagMapKuni rfKuni = this.search(nMapNo);
	if (rfKuni === NULL)
	{
		return -1;
	}
	int nOldColorNo = rfKuni.getColorNo();
	if (rfKuni.setColor(nColorNo))
	{
		rfKuni.setShow(true);
		return nOldColorNo;
	}
	return 0;
}

