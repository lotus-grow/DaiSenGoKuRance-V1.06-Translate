ref tagMapKuni tagMapKuniManager::search(int nMapNo)
{
	int cnt = this.m_asKuni.Numof();
	if (nMapNo < 1 || nMapNo > cnt)
	{
		return NULL;
	}
	return this.m_asKuni[nMapNo - 1];
}

